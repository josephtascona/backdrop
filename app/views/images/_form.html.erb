<%= form_for @image, html: { multipart: true } do |f| %>
  <div>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :image %><br />
    <%= f.file_field :image %>
  </div>
  <div>
    <%= f.label :category %><br />
    <%= f.text_field :category %>
  </div>
  <div>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </div>
  <%= f.hidden_field :user_id, value: current_user.id %>
  <div>
    <%= f.submit "Submit" %>
  </div>
<% end %>
